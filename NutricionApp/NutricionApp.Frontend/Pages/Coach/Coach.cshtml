@page
@model NutricionApp.Frontend.Pages.CoachModel
@{
}

<h1>Lista de pacientes</h1><br>
<!--para crear el boton que crea un nuevo coach.-->
<a class="btn btn-primary" asp.page="./Registro">Registrar nuevo Coach</a>
<br>
<br>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Apellidos</th>
      <th scope="col">Telefono</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var p in Model.Coaches)
    {
      <tr>
        <td>@p.Nombre</td>
        <td>@p.Apellidos</td>
        <td>@p.Telefono</td>
      </tr>
    }
  </tbody>
</table>
